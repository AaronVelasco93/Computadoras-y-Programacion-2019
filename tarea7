#include <stdio.h>
  
  int main(){
      
      int num1=0;
      int num2=0;
	  int num3=0;
      
	 printf("Dame el primer numero entero: ");
		scanf("%d",&num1);
	 printf("Dame el segundo numero entero: ");
		scanf("%d",&num2);
	 printf("Dame el tercer numero entero: ");
		scanf("%d",&num3);

		if (num1>0 && num2>0 && num3>0 && (num1 != num2 != num3)){

		 if (num1<num2)  { 
				 printf("%d"" el primer número es el menor\n",num1);

		 } else {
				 printf("%d" " el segundo número es el menor\n",num2);
		
		 }
				

		 }
		 
		 if (num3<num2)  {
		 	 printf("%d"" el tercer número es el menor\n",num3);

		 } 
			
		else{
              printf("los números deben ser mayores que cero y diferentes entre sí}");
			  		}	
		
				
}


		 		

    
		
