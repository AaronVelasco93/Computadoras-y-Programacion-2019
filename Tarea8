//4
#include <stdio.h>
   int main ()  {
   	
   	int meno=2;
   	int mayo=15;
    int j   =0;
    
      for (meno=2; meno<=mayo; meno=meno+1) {
       for(j=1; j<=10; j=j+1) {
	   
      	 printf("%d x %d = %d \n",meno,j,meno*j);
	  }  
	   
	   printf("+++++++++++++++++++\n\n");
	   
}
      printf ("fin del programa");
   }
