#include <stdio.h>
  
  int main(){
      
      int num1=0;
      float num2;
	  float num3;
      
	 printf("Dame el primer numero entero: ");
		scanf("%d",&num1);
	 printf("Dame el segundo numero : ");
		scanf("%f",&num2);
	 printf("Dame el tercer numero : ");
		scanf("%f",&num3);

		if (num1>0 && num2>0 && num3>0 && (num1 != num2 != num3)){

		 if (num1<num2)  { 
				 printf("%d"" el primer número es el menor\n",num1);

		 } else {
				 printf("%f" " el segundo número es el menor\n",num2);
		
		 }
				

		 }
		 
		 if (num3<num2)  {
		 	 printf("%f"" el tercer número es el menor\n",num3);

		 } 

}
